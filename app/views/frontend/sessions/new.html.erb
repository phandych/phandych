<div class="row">
	<div class="col-md-8"></div>
	<div class="col-md-4">
		<div id="user-form" class="form-margin">
			<div class="text-center">
				<h3>Login</h3>
			</div>
			<%= simple_form_for :session, url: login_path do |f| %>
				<%= f.input :email %>
				<%= f.input :password %>
				<%= f.button :submit, "Login" %>
			<% end %>
		</div>
	</div>
</div>