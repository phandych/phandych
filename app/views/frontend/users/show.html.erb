<div id="account" class="row">
	<div class="account-content">
		<div class="col-md-6">
			<div class="account-box">
				<span class="title">Account Information</span>
				<span><%= link_to "Edit", "#" %></span>
					
				<div class="divider"></div>
				<ul class="box-list">
					<li>User Name</li>
					<li>User Email</li>
					<li>Last Login</li>
				</ul>
			</div>
		</div>
		<div class="col-md-6">
			<div class="account-box">
				<span class="title">Store Information</span>
				<span><%= link_to "Edit", "#" %></span>
					
				<div class="divider"></div>
				<ul class="box-list">
					<li>Kecamatan, StoreID #</li>
					<li>Alamat</li>
					<li>Kontak</li>
					<li>Get Direction</li>
				</ul>
			</div>
		</div>
		<div class="col-md-6">
			<div class="account-box">
				<span class="title">Addresses</span>
				<span><%= link_to "Edit", "#" %></span> | <span><%= link_to "Create", "#" %></span>
					
				<div class="divider"></div>
				<ul class="box-list">
					<li>Nama Penerima</li>
					<li>Alamat</li>
					<li>Provinsi, Kecamatan, Kota, Kodepos</li>
					<li>Kontak</li>
				</ul>
			</div>
		</div>
		<div class="col-md-6">
			<div class="account-box">
				<span class="title">Newsletter</span>
				<span><%= link_to "Edit", "#" %></span>
					
				<div class="divider"></div>
				<p>Currently your not registered to our newsletter</p>
			</div>
		</div>
		<div class="col-md-12">
			<div class="logistic-box">
				<h3>Logistic Information</h3>
				<div class="divider"></div>
				<ul class="logistic-list">
					<li>
						<div class="logistic-info">
							<h5>INV/DATE/KODEPRODUKORSERVICE/INVNUMBER</h5>
							<span>Transaction Date: 31 July 2016 | Total: Rp. 200.000</span>
						</div>
						<div class=" text-right">
							<label>Status Pesanan</label>
							<label>Jenis Pesanan</label>
							<span><i class="fa fa-chevron-circle-down" aria-hidden="true"></i></span>
						</div>
					</li>
					<li>
						<div class="logistic-info">
							<h5>INV/DATE/KODEPRODUKORSERVICE/INVNUMBER</h5>
							<span>Transaction Date: 31 July 2016 | Total: Rp. 200.000</span>
						</div>
						<div class=" text-right">
							<label>Status Pesanan</label>
							<label>Jenis Pesanan</label>
							<span><i class="fa fa-chevron-circle-down" aria-hidden="true"></i></span>
						</div>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>